{
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 120,
    "proseWrap": "always",
    "vueIndentScriptAndStyle": false,
    "htmlWhitespaceSensitivity": "css",
    "quoteProps": "as-needed",
    "trailingComma": "none"
}
