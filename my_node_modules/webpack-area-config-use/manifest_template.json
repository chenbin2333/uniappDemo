{
    "name": "%CONFIG_APP_NAME%",
    "appid": "%CONFIG_APPID%",
    "description": "",
    "versionName": "%APP_VERSION_NAME%",
    "versionCode": "%APP_VERSION_CODE%",
    "transformPx": false,
    "networkTimeout": {
        "uploadFile": 300000
    },
    "mp-weixin": {
        "appid": "%CONFIG_MPWX_ID%",
        "setting": {
            "urlCheck": false,
            "minified": true,
            "postcss": false
        },
        "usingComponents": true,
        "requiredPrivateInfos": [
            "chooseLocation",
            "getLocation"
        ],
        "permission": {
            "scope.userLocation": {
                "desc": "您的定位信息将用于位置导航"
            }
        }
    },
    "h5": {
        "router": {
            "base": "%CONFIG_APP_BASE_URL%",
            "isTestEnv": "%CONFIG_IS_TEST_ENV%",
            "areaAppName": "%CONFIG_APP_AREA_NAME%",
            "areaConfigName": "%CONFIG_AREA_CONFIG_NAME%",
            "appBuildTime": "%APP_BUILD_TIME%",
            "appBuildHost": "%APP_BUILD_HOST%",
            "enableStat": true
        },
        "sdkConfigs": {
            "maps": {
                "qqmap": {
                    "key": "LPZBZ-PS3LX-NXD4Z-77MYJ-65POO-I2BI4"
                }
            }
        },
        "devServer": {
            "port": 8000,
            "disableHostCheck": true,
            "https": false,
            "proxy": {
                "/api": {
                    "target": "%CONFIG_APP_SERVER_PROXY%",
                    "changeOrigin": true,
                    "secure": false,
                    "pathRewrite": {
                        "^/api": ""
                    }
                }
            }
        },
        "uniStatistics": {
            "enable": true
        }
    },
    "vueVersion": "2"
}
